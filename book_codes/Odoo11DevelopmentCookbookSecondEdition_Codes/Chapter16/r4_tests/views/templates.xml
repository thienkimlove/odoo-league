<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link href="/r4_tests/static/src/css/r4_tests.css" rel="stylesheet" type="text/css"/>
            <script src="/r4_tests/static/src/js/r4_tests.js" type="text/javascript" />
        </xpath>
    </template>
    <template id="qunit_suite" inherit_id="web.qunit_suite">
        <xpath expr="//t[@t-set='head']" position="inside">
            <script src="/r4_tests/static/test/r4_tests.js" type="text/javascript" />
        </xpath>
    </template>
</odoo>

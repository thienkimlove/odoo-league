<odoo>
    <data>
        <template id="generation.layout" name="Generation layout" inherit_id="website.layout">

            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/css" href="/generation/static/frontend/css/bootstrap.css" media="all"/>
                <link rel="stylesheet" type="text/css" href="/generation/static/frontend/css/style.css" media="all"/>
                <link rel="stylesheet" type="text/css" href="/generation/static/frontend/css/font-awesome.min.css"
                      media="all"/>
                <link href='//fonts.googleapis.com/css?family=Poppins:300,400,600,400i,700' rel='stylesheet'
                      type='text/css'/>

                <link rel="stylesheet" href="/generation/static/frontend/js/owlcarousel/assets/owl.carousel.min.css"/>
                <link rel="stylesheet" href="/generation/static/frontend/js/owlcarousel/assets/owl.theme.default.min.css"/>
            </xpath>

            <xpath expr="//script[last()]" position="after">

                <script type="text/javascript" src="/generation/static/frontend/js/jquery.min.js" />
                <script type="text/javascript" src="/generation/static/frontend/js/bootstrap.min.js" />



                <script type="text/javascript" src="/generation/static/frontend/js/owlcarousel/owl.carousel.js" />
                <script type="text/javascript" src="/generation/static/frontend/js/owl.rows.js" />

                <script type="text/javascript" src="/generation/static/frontend/js/vendors/highlight.js" />
                <script type="text/javascript" src="/generation/static/frontend/js/app.js" />
                <script type="text/javascript" src="/generation/static/frontend/js/myscript.js" />

            </xpath>

            <xpath expr="//div[@id='wrapwrap']" position="replace">
                <header>
                    <div class="header-default">
                        <div class="container">
                            <div class="row no-margin">
                                <div class="col-md-3 col-sm-3 col-xs-12 no-padding">
                                    <div class="logo">
                                        <img src="/generation/static/frontend/images/logo.jpg" alt="Lao Premier League"/>
                                    </div>
                                </div>
                                <div class="col-md-9 col-sm-9 col-xs-12 no-padding">
                                    <div class="row topbar no-margin">
                                        <div class="col-md-8 col-sm-7 col-xs-12">
                                            <div class="block club-team">
                                                <div class="block-title">Clubs:</div>
                                                <div class="block-content">
                                                    <!-- Start clubs images carousel -->
                                                    <div class="owl-carousel owl-theme" id="clubs-carousel">
                                                        <div class="item">
                                                            <a href="#">
                                                                <img src="/generation/static/frontend/images/club01.png" alt="club01"/>
                                                            </a>
                                                        </div>
                                                        <div class="item">
                                                            <a href="#">
                                                                <img src="/generation/static/frontend/images/club02.png" alt="club01"/>
                                                            </a>
                                                        </div>
                                                        <div class="item">
                                                            <a href="#">
                                                                <img src="/generation/static/frontend/images/club01.png" alt="club01"/>
